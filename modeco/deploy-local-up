#!/usr/bin/env bash


prefix="package_$2"

if [[ "package_ksql_ksql_server_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create package_ksql_ksql_server_volume
	fi
	
if [[ "package_ksql_tocloud_ksql_server_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create package_ksql_tocloud_ksql_server_volume
	fi
	
